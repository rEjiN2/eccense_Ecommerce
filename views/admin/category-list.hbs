 <div class="main-panel">
          <div class="content-wrapper">
            <div class="row">
              <div class="col-lg-12 grid-margin stretch-card">
                <div class="card">
                  <div class="card-body">
                    <h4 class="card-title">Product List</h4>
                    <span>
                      <a href="/admin/add-product" class="btn btn-info" > Add Products
                    </a><a href="/admin/add-category" class="btn btn-success ml-5" > Add Category
                    </a><a href="/admin/add-user" class="btn btn-warning ml-5" > Add brand
                    </a>
                    </span>
                    <div class="table-responsive">
    <table class="table mt-5">
                        <thead>
                          <tr>
                            <th> Category Name</th>
                            <th>Sub Category</th>
                            <th>images</th>
                            <th>Options</th>
                          </tr>
                        </thead>
                        <tbody>
                          {{#each category}}
                          <tr>
                            <td>{{this.category}}</td>
                            <td>{{this.scategory}}</td>
                            <td><img src="/cat-images/{{this.catImage1}}" style="height: 60px; width:50px;" alt="">
                            </td>
                            <td><a href="/admin/edit-category?id={{this._id}}" class="btn btn-success">Edit</a>
                            <a href="/admin/delete-category?id={{this._id}}&name={{this.category}}" class="btn btn-danger" onclick="return confirm('Are u sure to delete {{this.category}}?')">Delete</a></td>
                            
                            
                          </tr>
                            {{/each}}
                        </tbody>
                      </table>
                      </div>
                  </div>
                </div>
              </div>
              </div>
              </div>

                      


    

