
 <div class="main-panel">
          <div class="content-wrapper">
            <div class="row">
              <div class="col-lg-12 grid-margin stretch-card">
                <div class="card">
                  <div class="card-body">
                    <h4 class="card-title">Product List</h4>
                    <span>
                      <a href="/admin/add-product" class="btn btn-info" > Add Products
                    </a><a href="/admin/add-category" class="btn btn-success ml-5" > Add Category
                    </a><a href="/admin/add-user" class="btn btn-warning ml-5" > Add brand
                    </a>
                    </span>
                    <div class="table-responsive">
    <table id="productsTable" class="table mt-5">
                        <thead>
                          <tr>
                           
                        {{!-- <th>Customer</th> --}}
						<th>Name</th>
            <th>Date</th>						
            <th>Status</th>						
						<th> Amount</th>
            <th>Payment</th>
						<th>Products</th>
            <th>Options</th>
						
                          </tr>
                        </thead>
                        <tbody>
                          {{#each orderList}}
                          <tr>
                            
                            <td>{{this.deliveryDetails.name}}</td>
                            <td>{{this.date}}</td>
                            <td>{{this.status}}</td>
                            <td>{{this.totalAmount}}</td>
                            <td>{{this.paymentMethod}}</td>
                            <td><a href='/admin/viewOrder?id={{this._id}}' class="btn btn-primary">View Products</a></td>
                            <td><a href='/admin/viewOrder?id={{this._id}}' class="btn btn-primary">Approve</a></td>
                          </tr>
                          {{/each}}
                        </tbody>
                      </table>
                      </div>
                  </div>
                </div>
              </div>
              </div>
              </div>
